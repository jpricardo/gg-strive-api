{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",
    "rootDir": "./",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true
  }, 
  "exclude": ["./src/client", "./src/**/*.test.ts"]
}
